{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:parrot",
      "materials": { "default": "entity_alphatest"},
      "textures": {
        "blue": "textures/custom/peoplemover/peoplemover_blue",
        "green": "textures/custom/peoplemover/peoplemover_blue",
        "red_blue": "textures/custom/peoplemover/peoplemover_blue",
        "yellow_blue": "textures/custom/peoplemover/peoplemover_blue",
        "grey": "textures/custom/peoplemover/peoplemover_blue"
      },
      "geometry": {
        "default": "geometry.peoplemover"
      },
      "render_controllers": [ "controller.render.parrot" ],
      "spawn_egg": {
        "texture": "spawn_egg",
        "texture_index": 43
      },
      "animations": {
        "rotate": "animation.generic.rotate"
			},
      "scripts": {
        "initialize": [
          "variable.slope = 0.0;"
        ],
				"pre_animation": [
          "variable.slope_calc = query.vertical_speed / query.ground_speed;",
          "variable.slope = math.abs(variable.slope-variable.slope_calc)>0.06 ? (variable.slope-variable.slope_calc > 0 ? variable.slope-0.03 : variable.slope+0.03) : variable.slope;"
        ],
				"animate": [
					"rotate"
				]
			}
    }
  }
}
