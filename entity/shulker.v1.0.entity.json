        "white": "textures/custom/caseyjrcircustrain/casey_caboose",
        "orange": "textures/custom/caseyjrcircustrain/casey_caboose",
        "magenta": "textures/custom/caseyjrcircustrain/casey_caboose",
        "light_blue": "textures/custom/caseyjrcircustrain/casey_caboose",
        "yellow": "textures/custom/caseyjrcircustrain/casey_caboose",
        "lime": "textures/custom/caseyjrcircustrain/casey_caboose",
        "pink": "textures/custom/caseyjrcircustrain/casey_caboose",
        "gray": "textures/custom/caseyjrcircustrain/casey_caboose",
        "silver": "textures/custom/caseyjrcircustrain/casey_caboose",
        "cyan": "textures/custom/caseyjrcircustrain/casey_caboose",
        "purple": "textures/custom/caseyjrcircustrain/casey_caboose",
