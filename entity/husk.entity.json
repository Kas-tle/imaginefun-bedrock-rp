{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:husk",
      "min_engine_version": "1.8.0",
      "materials": { "default": "entity_alphatest" },
      "textures": { "default": "textures/custom/caseyjrcircustrain/casey_monkeys" },
      "geometry": { "default": "geometry.casey_monkeys" },
      "enable_attachables": false,
      "spawn_egg": {
        "texture": "spawn_egg",
        "texture_index": 28
      },
      "scripts": {
        "initialize": [
          "variable.slope = 0.0;"
        ],
				"pre_animation": [
          "variable.slope_calc = query.vertical_speed / query.ground_speed;",
          "variable.slope = math.abs(variable.slope-variable.slope_calc)>0.06 ? (variable.slope-variable.slope_calc > 0 ? variable.slope-0.03 : variable.slope+0.03) : variable.slope;"
        ],
				"animate": [
					"rotate"
				]
			}
    }
  }
}
