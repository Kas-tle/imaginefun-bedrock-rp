{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:wither_skeleton",
      "materials": { "default": "entity_alphatest" },
      "textures": { "default": "textures/custom/grizzlyriverrun/grr_raft1" },
      "geometry": { "default": "geometry.grr_raft1" },
      "render_controllers": [ "controller.render.wither_skeleton" ],
      "spawn_egg": {
        "texture": "spawn_egg",
        "texture_index": 29
      },
      "enable_attachables": false,
      "animations": {
        "rotate": "animation.body.rotate"
			},
      "scripts": {
        "initialize": [
          "variable.slope = 0.0;"
        ],
				"pre_animation": [
          "variable.slope_calc = query.vertical_speed / query.ground_speed;",
          "variable.slope = math.abs(variable.slope-variable.slope_calc)>0.06 ? (variable.slope-variable.slope_calc > 0 ? variable.slope-0.03 : variable.slope+0.03) : variable.slope;"
        ],
				"animate": [
					"rotate"
				]
			}
    }
  }
}
